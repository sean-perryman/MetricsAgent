{
  "interval_seconds": 30,
  "timeout_seconds": 10,
  "hostname_override": "",
  "metrics": {
    "cpu": true,
    "memory": true,
    "disk": true,
    "volumes": true,
    "network": true,
    "users": true
  },
  "exporters": {
    "json": {
      "enabled": true,
      "endpoint": "https://metrics.example.com/ingest",
      "api_key": "REPLACE_ME"
    },
    "prometheus": {
      "enabled": false,
      "pushgateway": "http://localhost:9091",
      "job": "metricsagent"
    },
    "otlp": {
      "enabled": false,
      "endpoint": "https://otel.example.com/v1/metrics"
    }
  }
}